{
    "builds": [
        {
          "src": "next.config.js",
          "use": "@vercel/next"
        },
        {
          "src": "static/**",
          "use": "@vercel/static"
        }
    ],
    "presets": ["next/babel"],
    "plugins": [
        [
            "babel-plugin-styled-components",
            {
                "ssr": true,
                "displayName": true,
                "fileName": true
            }
        ]
    ]
}
  